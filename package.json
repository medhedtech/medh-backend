{
  "name": "medh-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "debug": "NODE_ENV=development nodemon --inspect index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --testPathIgnorePatterns=tests/zoom.test.js --passWithNoTests",
    "test:zoom": "NODE_OPTIONS=--experimental-vm-modules jest --config tests/zoom.test.config.js tests/zoom.test.js",
    "test:api": "node tests/api-test.js",
    "test:api:debug": "DEBUG=true node tests/api-test.js",
    "test:api:auto": "node tests/run-api-tests.js",
    "test:api:report": "start reports/api-test-report.html",
    "test:postman": "./test-api.sh",
    "test:refresh-token": "node test-refresh-token.js",
    "lint": "eslint . --quiet",
    "format": "prettier --write .",
    "logs:error": "tail -f logs/error-*.log",
    "logs:combined": "tail -f logs/combined-*.log",
    "logs:exceptions": "tail -f logs/exceptions-*.log",
    "audit": "npm audit --production",
    "security-check": "snyk test",
    "build": "npm run lint && npm run test",
    "prepare": "node .husky/setup.js"
  },
  "jest": {
    "transform": {},
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testEnvironment": "node"
